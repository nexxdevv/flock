{
  "expo": {
    "name": "flock",
    "slug": "flock",
    "version": "1.0.0",
    "platforms": ["ios"],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "bundleIdentifier": "com.nexxdevv.flock",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.384055557207-r1296h1edumdh3gtom4a0dqeuirqkaup "
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nexxdevv.flock"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "flock"
  }
}
